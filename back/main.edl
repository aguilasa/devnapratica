domain furb

entities {
	generateExtraResources : true
    securityType : secure
	enumType : ordinal
}

service basico {
	entities {
		entity product {
			relationships{
        		ManyToOne(category) {
        			type: aggregation
    				fetchType: eager
        		}
        	}
        }
		entity itemList {
			relationships{
        		ManyToOne(product) {
        			type: aggregation
    				fetchType: eager
        		}
        	}
        }
		entity shoppingList {
			relationships{
                OneToMany(items){
                    type: aggregation
                }
            }
        }
	}
}
