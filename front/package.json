{
    "name": "basico-frontend",
    "version": "1.0.0",
    "scripts": {
        "start": "node server.js",
        "build": "ng build --prod",
        "build:stats": "npm run build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
        "test": "ng test --watch=false",
        "test:dev": "ng test",
        "test:coverage": "ng test --watch=false --code-coverage && npx http-server coverage -o",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "project": {
        "app": "furb-basico",
        "domain": "furb",
        "service": "basico",
        "serviceDependencies": [
            {
                "service": "basico",
                "version": "develop-SNAPSHOT"
            }
        ]
    },
    "keywords": [
        "ES8",
        "JavaScript",
        "TypeScript",
        "Angular",
        "Senior"
    ],
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.1",
        "@angular/cli": "~7.1.3",
        "@angular/compiler-cli": "~7.1.3",
        "@angular/language-service": "~7.1.3",
        "@types/jasmine": "2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.3.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.3",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "~0.2.2",
        "karma-spec-reporter": "0.0.32",
        "protractor": "~5.4.1",
        "ts-node": "~5.0.1",
        "tsconfig-paths": "~3.3.2",
        "tslint": "~5.9.1",
        "typescript": "~3.1.6"
    },
    "dependencies": {
        "@angular/animations": "~7.1.3",
        "@angular/cdk": "~7.1.1",
        "@angular/common": "~7.1.3",
        "@angular/compiler": "~7.1.3",
        "@angular/core": "~7.1.3",
        "@angular/forms": "~7.1.3",
        "@angular/http": "~7.1.3",
        "@angular/platform-browser": "~7.1.3",
        "@angular/platform-browser-dynamic": "~7.1.3",
        "@angular/router": "~7.1.3",
        "@ngx-translate/core": "~9.0.2",
        "@ngx-translate/http-loader": "~2.0.1",
        "@seniorsistemas/senior-platform-data": "~2.0.2",
        "@seniorsistemas/primeng-theme": "~5.0.2",
        "@seniorsistemas/angular-components": "~9.0.0",
        "@seniorsistemas/platform-components": "~2.0.1",
        "angular2-hotkeys": "~2.1.2",
        "core-js": "~2.5.7",
        "font-awesome": "~4.7.0",
        "moment": "~2.22.2",
        "ng2-currency-mask": "~5.3.1",
        "primeng": "~7.0.0",
        "rxjs": "~6.3.3",
        "rxjs-compat": "~6.2.2",
        "zone.js": "~0.8.26"
    }
}
